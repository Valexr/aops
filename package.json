{
    "name": "@valexr/aops",
    "description": "Animate on Page Scroll",
    "version": "0.3.0",
    "unpkg": "dist/index.js",
    "jsdelivr": "dist/index.js",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/*"
    ],
    "type": "module",
    "scripts": {
        "build": "node esbuild",
        "dev": "node esbuild --dev --spa",
        "preview": "npx esbuild --servedir=public",
        "dist": "node esbuild --dist",
        "pub": "npm run dist && npm publish --access=public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Valexr/aops.git"
    },
    "devDependencies": {
        "@types/node": "^22.7.4",
        "@valexr/aops": "^0.3.0",
        "esbuild": "^0.23.1",
        "esbuild-svelte": "^0.8.2",
        "svelte": "^4.2.19",
        "svelte-preprocess": "^6.0.3",
        "typescript": "^5.6.2"
    }
}