{
    "name": "@valexr/aops",
    "description": "Animate on Page Scroll",
    "version": "0.1.3",
    "unpkg": "dist/index.js",
    "jsdelivr": "dist/index.js",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/*"
    ],
    "type": "module",
    "scripts": {
        "build": "node esbuild",
        "dev": "node esbuild --dev --spa",
        "preview": "npx esbuild --servedir=public",
        "dist": "node esbuild --dist",
        "pub": "npm run dist && npm publish --access=public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Valexr/aops.git"
    },
    "devDependencies": {
        "@types/node": "^20.14.10",
        "@valexr/aops": "^0.1.3",
        "esbuild": "^0.23.0",
        "esbuild-svelte": "^0.8.1",
        "svelte": "^4.2.18",
        "svelte-preprocess": "^6.0.2",
        "typescript": "^5.5.3"
    }
}